{
    "name": "react-starter",
    "version": "1.0.0",
    "description": "A starter kit, boilerplate for building React single page applications with Webpack 2, Babel, and Yarn",
    "main": "index.js",
    "repository": "https://github.com/npranto/react-guide.git",
    "author": "Nazmuz Shakib Pranto <npranto@gmail.com>",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist/",
        "prod": "NODE_ENV=production yarn run clean && node ./node_modules/webpack/bin/webpack.js --config=webpack.prod.config.js -p",
        "build": "yarn run clean && node ./node_modules/webpack/bin/webpack.js",
        "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base dist/ --history-api-fallback"
    },
    "devDependencies": {
        "babel-core": "^6.2.1",
        "babel-loader": "^6.2.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-react": "^6.23.0",
        "css-loader": "^0.28.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "favicons-webpack-plugin": "^0.0.7",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "image-webpack-loader": "^3.3.0",
        "jsdom": "^8.1.0",
        "react-addons-test-utils": "^15.5.1",
        "rimraf": "^2.6.1",
        "style-loader": "^0.16.1",
        "url-loader": "^0.5.8",
        "webpack": "^2.3.3",
        "webpack-dev-server": "^2.4.2"
    },
    "dependencies": {
        "react": "^15.4.2",
        "react-dom": "^15.4.2"
    }
}
